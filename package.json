  "scripts": {
    "clean:modules": "rm -Rf node_modules yarn.lock && yarn cache clean && yarn",
    "generate:bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && yarn start:android",
    "new:component": "cross-env ./scripts/new_component.sh",
    "open:emulator": "cross-env TODO:",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "react-native run-android",
    "test": "yarn test:lint && yarn test:jest",
    "test:codecov": "./node_modules/.bin/codecov",
    "test:jest": "jest",
    "test:lint": "eslint ./",
    "test:lint:fix": "eslint --ext .js --fix src",
    "test:snyk": "snky test",
    "test:jest:watch": "jest --watch"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
